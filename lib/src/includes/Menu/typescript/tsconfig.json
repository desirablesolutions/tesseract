{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "moduleResolution": "node",
      "declaration": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "outDir": "dist",
      "rootDir": "src",
      "baseUrl": ".",
      "paths": {
        "src/*": ["src/*"],
        "@views/*":["src/views/*"]
      }
    },
    "include": ["src"]
  }
  