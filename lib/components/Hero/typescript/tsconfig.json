{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@presets/*": ["./src/presets/*"],
        "@typings/*": ["./src/typings/*"]

      }
    },
    "include": ["src"]
  }
  